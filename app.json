{
  "expo": {
    "name": "KnitKit - Knitting Companion",
    "slug": "knitkit-knitting-companion",
    "version": "1.0.0",
    "orientation": "portrait",

    "scheme": "rork-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "icon": "./assets/app-icon.png",

    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.knitkit-knitting-companion",
      "icon": "./assets/app-icon.png"
    },

    "android": {
      "package": "app.rork.knitkit_knitting_companion",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/app-icon.png"
    },

    "web": {
      "favicon": "./web/icons/icon-192.png",
      "bundler": "metro",
      "output": "static"
    },

    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}